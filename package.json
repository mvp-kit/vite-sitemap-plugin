{
  "name": "@mvp-kit/vite-sitemap-plugin",
  "version": "0.0.3",
  "description": "Vite plugin for automatic sitemap generation from TanStack Router route tree",
  "type": "module",
  "main": "dist/index.js",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup src/index.ts --format esm --dts",
    "dev": "tsup src/index.ts --format esm --dts --watch",
    "typecheck": "tsc --noEmit",
    "test": "vitest",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "vite",
    "plugin",
    "sitemap",
    "seo",
    "tanstack-router",
    "react-router",
    "typescript",
    "build-tool",
    "automation",
    "xml-sitemap",
    "robots-txt"
  ],
  "author": "MVPKit",
  "license": "MIT",
  "peerDependencies": {
    "vite": "^7.0.0"
  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "tsup": "^8.0.0",
    "typescript": "^5.0.0",
    "vite": "^7.0.0",
    "vitest": "^2.0.0"
  },
  "engines": {
    "node": ">=18"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mvp-kit/vite-sitemap-plugin.git"
  },
  "homepage": "https://github.com/mvp-kit/vite-sitemap-plugin#readme",
  "bugs": {
    "url": "https://github.com/mvp-kit/vite-sitemap-plugin/issues"
  },
  "publishConfig": {
    "access": "public"
  }
}
